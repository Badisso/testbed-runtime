#!/bin/bash

if [ "$#" -lt 2 ]
then
	echo "Usage $0 PROPERTIES_FILE SECRET_RESERVATION_KEYS [NODEURNS]"
	echo "Example: $0 live.properties urn:wisebed:uzl1:,D2C88AEE416EDD2FD4038BC1252A8900 urn:wisebed:uzl1:0xf859,urn:wisebed:uzl1:0xf860"
	exit 1
fi

java -Dtestbed.secretreservationkeys=$2 -Dtestbed.nodeurns=$3 -jar ../wisebed-cmdline-client/target/tr.wisebed-cmdline-client-0.6-SNAPSHOT.one-jar.jar -p $1 -f check-alive-protobuf.java
