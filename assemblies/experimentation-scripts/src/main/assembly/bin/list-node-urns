#!/bin/bash
BASEDIR=`dirname $0`

if [ "$#" -lt 1 ]
then
	echo "Usage $0 PROPERTIES_FILE [NODE_TYPES] [csv|lines]"
	echo "Example: $0 live.properties isense,telosb csv"
	echo "CSV is the default listing type"
	exit 1
fi

java \
 -Dtestbed.nodetypes=$2 \
 -Dtestbed.listtype=$3 \
 -jar $BASEDIR/../lib/tr.scripting-client-${project.version}-onejar.jar \
 -p $1 \
 -f $BASEDIR/../scripts/list-node-urns.java
